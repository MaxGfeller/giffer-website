  "dependencies": {
    "brfs": "^1.2.0",
    "concat-stream": "^1.4.6",
    "dnode": "^1.2.0",
    "giffer": "^1.3.0",
    "giffer-thumbnail": "^1.0.0",
    "giffer-adapter-9gag": "^1.0.0",
